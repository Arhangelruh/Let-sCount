{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "NLog": {
    "targets": {
      "file": {
        "type": "File",
        "fileName": "D:\\test\\logs\\app-${date:format=yyyy-MM-dd}.log",
        "layout": "${longdate}|${level:uppercase=true}|${logger}|${message} ${exception}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "file"
      }
    ]
  },
  "ConnectionStrings": {
    "LetUsCountDatabase": ""
  }
}
